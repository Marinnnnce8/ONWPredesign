	<main class="uk-margin-bottom">

		{{> sightingButton}}

		{{#if page.noBannerBreadcrumbs}}
		{{else}}
			{{#if page.banner}}
			{{> banner}}
			{{#if page.breadcrumbs}}
			{{#> ukContainer size=(default page.ukContainer ukContainer) class="uk-margin-top"}}
			{{#*inline "out"}}
			{{> ukBreadcrumb }}
			{{/inline}}
			{{/ukContainer}}
			{{/if}}
			{{else}}
			{{#> ukContainer size=(default page.ukContainer ukContainer) class="uk-margin-top"}}
			{{#*inline "out"}}
			<div class="no-hero">
				<h1>{{page.h1}}</h1>
			</div>
			{{> ukBreadcrumb }}
			{{/inline}}
			{{/ukContainer}}
			{{/if}}
		{{/if}}

		{{#> main }}{{/main}}

	</main>

	<footer class="footer uk-position-relative">
		<img class="uk-visible@l" src="./img/bird-intro.png" data-uk-img alt="image">
		{{#> ukContainer}}
		{{#*inline "out"}}
